{
  "plugins": ["plugins/markdown", "plugin/assertivedocs"],
  "recurseDepth": 10,
  "source": {
    "include": ["./plugin", "./demo"],
    "exclude": ["./node_modules"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "layoutFile": "./plugin/layout.tmpl"
    }
  },
  "opts": {
    "destination": "./docs"
  }
}